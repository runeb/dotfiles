[github]
  user = <%= print("Github Username: "); STDOUT.flush; STDIN.gets.chomp %>
  token = <%= print("Github Token: "); STDOUT.flush; STDIN.gets.chomp %>
[user]
  name = <%= print("Your Name: "); STDOUT.flush; STDIN.gets.chomp %>
  email = <%= print("Your Email: "); STDOUT.flush; STDIN.gets.chomp %>
[core]
  excludesfile = <%= ENV['HOME'] %>/.gitignore
  editor = mvim 
[color]
  diff = auto
  branch = auto
  status = auto
[alias]
  co = checkout
  st = status
  d = diff
  ci = commit -v
  cia = commit -v -a
  co = checkout
  cp = cherry-pick
  l = log
  ll = log -p
  lm = log master..
  llm = log -p master..
  b = branch
